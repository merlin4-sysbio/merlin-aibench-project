	<modules>
	  <module>aibench-aibench</module>
	  <module>aibench-core</module>
	  <module>aibench-workbench</module>
	  <module>aibench-shell</module>
	  <module>aibench-pluginmanager</module>
	  <module>aibench-archetype</module>
	</modules>
	
</project>
